@model IEnumerable<GCD0803TodoManagement.Models.ApplicationUser>
@{
	ViewBag.Title = "Members";
}

<h2>Members in Team</h2>

<div class="row">
	<div class="col-md-4">
		<ul class="list-group">
			<li class="list-group-item active">
			Members in Team
				<span class="badge">@Model.Count()</span>
			</li>
			@foreach (var user in Model)
			{
				<li class="list-group-item">@user.Email</li>
			}
		</ul>
	</div>
</div>

